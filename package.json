{
  "name": "@good-for-you-web-services/standard-components",
  "description": "A set of standard react components",
  "private": false,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/good-for-you-web-services/standard-components.git"
  },
  "homepage": "https://github.com/good-for-you-web-services/standard-components#readme",
  "bugs": {
    "url": "https://github.com/good-for-you-web-services/standard-components/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    },
    "./utilities/*": {
      "types": "./dist/utilities/*/index.d.ts",
      "import": "./dist/utilities/*/index.js",
      "require": "./dist/utilities/*/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "bootstrap-icons": "^1.11.3",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "modern-normalize": "^3.0.1",
    "sass": "^1.87.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-svgr": "^4.3.0"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "directories": {
    "lib": "lib"
  },
  "packageManager": "pnpm@10.6.4"
}
